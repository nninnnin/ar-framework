{
  "name": "@ar-framework/controls",
  "packageManager": "yarn@4.6.0",
  "dependencies": {
    "@ar-framework/utils": "workspace:^",
    "clsx": "*",
    "dotenv": "*",
    "immer": "^10.1.1",
    "mapbox-gl": "^3.10.0",
    "radix-ui": "^1.1.3",
    "react": "*",
    "react-dom": "*",
    "tailwindcss": "^4.0.6",
    "zustand": "*"
  },
  "scripts": {
    "copyAppToWebPublic": "cp -r dist/index.js ../../web/public/scripts/controls.js && cp -r dist/index.css ../../web/public/styles/controls.css",
    "build": "parcel build index.html --no-content-hash --no-source-maps && yarn copyAppToWebPublic",
    "start": "parcel index.html"
  },
  "devDependencies": {
    "@parcel/config-default": "*",
    "@parcel/core": "*",
    "@types/node": "^22.13.4",
    "@types/react": "^19.0.8",
    "parcel": "*",
    "parcel-namer-hashless": "*",
    "postcss": "^8.2.1"
  }
}
