    "start": "parcel index.html"
  },
  "devDependencies": {
    "@parcel/config-default": "^2.13.3",
    "@parcel/core": "^2.13.3",
    "@types/react": "^19.0.8",
    "parcel": "^2.13.3",
    "parcel-namer-hashless": "^1.0.7",
    "postcss": "^8.2.1"
  }
}
