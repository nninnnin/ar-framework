{
  "name": "@ar-framework/capturer",
  "packageManager": "yarn@4.6.0",
  "version": "1.0.0",
  "scripts": {
    "copyAppToWebPublic": "cp -r dist/index.js ../../web/public/features/capturer/index.js && cp -r dist/index.css ../../web/public/features/capturer/index.css",
    "build": "parcel build src/index.ts --no-content-hash --no-source-maps && yarn copyAppToWebPublic",
    "start": "parcel index.ts"
  },
  "dependencies": {
    "@types/three": "0.152.0",
    "clsx": "*",
    "dotenv": "*",
    "react": "*",
    "react-dom": "*",
    "three": "0.152.0",
    "zustand": "*"
  },
  "devDependencies": {
    "@parcel/config-default": "*",
    "@parcel/core": "*",
    "parcel": "*",
    "parcel-namer-hashless": "*"
  }
}
